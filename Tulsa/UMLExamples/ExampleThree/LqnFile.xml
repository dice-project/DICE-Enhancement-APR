            <activity bound-to-entry="UsageScenario_Entry" host-demand-mean="0.0" name="Start"/>
            <activity host-demand-mean="0.0" name="WikiArticleSpout">
               <synch-call calls-mean="1.0" dest="WikiArticleSpoutDest"/>
            </activity>
            <activity host-demand-mean="0.0" name="CategoriesExtractBolt">
