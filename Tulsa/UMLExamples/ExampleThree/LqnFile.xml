               <synch-call calls-mean="1.0" dest="LinksPerPageCassandraWriterDest"/>
            </activity>
            <activity name="Stop"/>
            <precedence>
               <pre>
                  <activity name="Start"/>
               </pre>
               <post>
                  <activity name="WikiArticleSpout"/>
               </post>
            </precedence>
            <precedence>
               <pre>
                  <activity name="WikiArticleSpout"/>
               </pre>
               <post-OR>
                  <activity name="CategoriesExtractBolt" prob="0.5"/>
                  <activity name="LinkCounterBolt" prob="0.5"/>
               </post-OR>
            </precedence>
            <precedence>
               <pre>
                  <activity name="CategoriesExtractBolt"/>
               </pre>
               <post-OR>
                  <activity name="CategoriesSplitBolt" prob="0.5"/>
                  <activity name="CategoriesPerPageCassandraWriter" prob="0.5"/>
               </post-OR>
            </precedence>
            <precedence>
