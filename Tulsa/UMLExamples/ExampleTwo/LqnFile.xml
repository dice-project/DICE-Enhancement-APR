<?xml version="1.0" encoding="UTF-8"?>
<lqn-model xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="RootElement To lqn"
           xsi:noNamespaceSchemaLocation="/usr/local/share/lqns/lqn.xsd">
   <processor multiplicity="2" name="Cluster" quantum="0.001" scheduling="fcfs"
              speed-factor="1.0">
      <task activity-graph="YES" multiplicity="1" name="Cluster_Task" scheduling="inf"
            think-time="0.0">
         <entry name="SentenceSpoutEntry" type="PH1PH2">
            <entry-phase-activities>
               <activity host-demand-mean="3.1" name="SentenceSpoutActivity" phase="1"/>
            </entry-phase-activities>
         </entry>
         <entry name="SplitBoltEntry" type="PH1PH2">
            <entry-phase-activities>
               <activity host-demand-mean="45.0" name="SplitBoltActivity" phase="1"/>
            </entry-phase-activities>
         </entry>
         <entry name="WordCountBoltEntry" type="PH1PH2">
            <entry-phase-activities>
               <activity host-demand-mean="2.0" name="WordCountBoltActivity" phase="1"/>
            </entry-phase-activities>
         </entry>
      </task>
   </processor>
   <processor multiplicity="1" name="UsageScenario_Processor" scheduling="fcfs">
      <task multiplicity="1" name="UsageScenario_Task" scheduling="ref" think-time="0.2">
         <entry name="UsageScenario_Entry" type="none">
            <service-time-distribution max="10.0" min="0.0" phase="1"/>
         </entry>
         <task-activities>
            <activity bound-to-entry="UsageScenario_Entry" host-demand-mean="0.0" name="Start"/>
            <activity host-demand-mean="0.0" name="SentenceSpoutActivity">
               <synch-call calls-mean="1.0" dest="SentenceSpout"/>
            </activity>
            <activity host-demand-mean="0.0" name="SplitBoltActivity">
               <synch-call calls-mean="1.0" dest="SplitBolt"/>
            </activity>
            <activity host-demand-mean="0.0" name="WordCountBoltActivity">
               <synch-call calls-mean="1.0" dest="WordCountBolt"/>
            </activity>
            <activity name="Stop"/>
            <precedence>
               <pre>
                  <activity name="Start"/>
               </pre>
               <post>
                  <activity name="SentenceSpoutActivity"/>
               </post>
            </precedence>
            <precedence>
               <pre>
                  <activity name="SentenceSpoutActivity"/>
               </pre>
               <post>
                  <activity name="SplitBoltActivity"/>
               </post>
            </precedence>
            <precedence>
               <pre>
                  <activity name="SplitBoltActivity"/>
               </pre>
               <post>
                  <activity name="WordCountBoltActivity"/>
               </post>
            </precedence>
            <precedence>
               <pre>
                  <activity name="WordCountBoltActivity"/>
               </pre>
               <post>
                  <activity name="Stop"/>
               </post>
            </precedence>
         </task-activities>
      </task>
   </processor>
   <processor multiplicity="1" name="SentenceSpoutProcessor" scheduling="fcfs">
      <task multiplicity="1" name="SentenceSpoutTask" scheduling="inf" think-time="0.0">
         <entry name="SentenceSpout" type="none"/>
         <task-activities>
            <activity bound-to-entry="SentenceSpout" host-demand-mean="0.0" name="Start0"/>
            <activity host-demand-mean="0.0" name="SentenceSpoutActivity">
               <synch-call calls-mean="1.0" dest="SentenceSpoutEntry"/>
            </activity>
            <activity name="Stop0"/>
            <precedence>
               <pre>
                  <activity name="Start0"/>
               </pre>
               <post>
                  <activity name="SentenceSpoutActivity"/>
               </post>
            </precedence>
            <precedence>
               <pre>
                  <activity name="SentenceSpoutActivity"/>
               </pre>
               <post>
                  <activity name="Stop0"/>
               </post>
            </precedence>
            <reply-entry name="SentenceSpout">
               <reply-activity name="Stop0"/>
            </reply-entry>
         </task-activities>
      </task>
   </processor>
   <processor multiplicity="1" name="SplitBoltProcessor" scheduling="fcfs">
      <task multiplicity="1" name="SplitBoltTask" scheduling="inf" think-time="0.0">
         <entry name="SplitBolt" type="none"/>
         <task-activities>
            <activity bound-to-entry="SplitBolt" host-demand-mean="0.0" name="Start1"/>
            <activity host-demand-mean="0.0" name="SplitBoltActivity">
               <synch-call calls-mean="1.0" dest="SplitBoltEntry"/>
            </activity>
            <activity name="Stop1"/>
            <precedence>
               <pre>
                  <activity name="Start1"/>
               </pre>
               <post>
                  <activity name="SplitBoltActivity"/>
               </post>
            </precedence>
            <precedence>
               <pre>
                  <activity name="SplitBoltActivity"/>
               </pre>
               <post>
                  <activity name="Stop1"/>
               </post>
            </precedence>
            <reply-entry name="SplitBolt">
               <reply-activity name="Stop1"/>
            </reply-entry>
         </task-activities>
      </task>
   </processor>
   <processor multiplicity="1" name="WordCountBoltProcessor" scheduling="fcfs">
      <task multiplicity="1" name="WordCountBoltTask" scheduling="inf" think-time="0.0">
         <entry name="WordCountBolt" type="none"/>
         <task-activities>
            <activity bound-to-entry="WordCountBolt" host-demand-mean="0.0" name="Start2"/>
            <activity host-demand-mean="0.0" name="WordCountBoltActivity">
               <synch-call calls-mean="1.0" dest="WordCountBoltEntry"/>
            </activity>
            <activity name="Stop2"/>
            <precedence>
               <pre>
                  <activity name="Start2"/>
               </pre>
               <post>
                  <activity name="WordCountBoltActivity"/>
               </post>
            </precedence>
            <precedence>
               <pre>
                  <activity name="WordCountBoltActivity"/>
               </pre>
               <post>
                  <activity name="Stop2"/>
               </post>
            </precedence>
            <reply-entry name="WordCountBolt">
               <reply-activity name="Stop2"/>
            </reply-entry>
         </task-activities>
      </task>
   </processor>
</lqn-model>